{
  "name": "@graphique/datasets",
  "version": "0.0.4-alpha.11",
  "description": "A collection of datasets used in Graphique examples",
  "keywords": [
    "react",
    "d3",
    "data-visualization",
    "grammar-of-graphics",
    "graphique",
    "dataset"
  ],
  "files": [
    "/src",
    "/dist"
  ],
  "type": "module",
  "module": "./dist/index.modern.js",
  "source": "./src/index.ts",
  "exports": "./dist/index.modern.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/graphiquejs/graphique.git",
  "author": "Matt Adams",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint ./ --fix",
    "prepack": "yarn run lint && yarn run build",
    "build": "microbundle -f modern --compress -o dist/index.js --sourcemap false",
    "watch": "microbundle watch -f modern"
  },
  "devDependencies": {
    "microbundle": "^0.13.3"
  }
}
