{
  "name": "@graphique/geom-line",
  "version": "0.0.2-alpha.3",
  "description": "For line charts and other lines",
  "keywords": [
    "react",
    "d3",
    "data-visualization",
    "grammar-of-graphics",
    "graphique",
    "geom-line",
    "line-chart"
  ],
  "source": "./src/index.ts",
  "exports": "./dist/index.modern.js",
  "main": "./dist/index.cjs",
  "module": "./dist/index.module.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "repository": "https://github.com/graphiquejs/graphique.git",
  "author": "Matt Adams",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint ./ --fix",
    "prepack": "yarn run lint && yarn run build",
    "build": "microbundle build --jsx React.createElement --jsxFragment React.Fragment -f es,cjs,modern",
    "watch": "microbundle watch --jsx React.createElement --jsxFragment React.Fragment -f es,cjs,modern"
  },
  "dependencies": {
    "d3-array": "^3.0.1",
    "d3-ease": "^3.0.1",
    "d3-interpolate": "^3.0.1",
    "d3-interpolate-path": "^2.2.3",
    "d3-shape": "^3.0.1",
    "jotai": "^1.1.2",
    "react-move": "^6.5.0"
  },
  "peerDependencies": {
    "@graphique/graphique": ">=^0.0.2",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "devDependencies": {
    "@types/d3-array": "^3.0.1",
    "@types/d3-ease": "^3.0.0",
    "@types/d3-interpolate": "^3.0.1",
    "@types/d3-interpolate-path": "^2.0.0",
    "@types/d3-shape": "^3.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0"
  }
}
